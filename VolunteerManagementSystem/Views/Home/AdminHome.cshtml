@model ViewModel

@{
    Layout = null;
}
<!DOCTYPE html>
<html>
<head>
    <link href="~/css/Style/StyleSheet1.css" rel="stylesheet" type="text/css" />
</head>
<body class="center" style="background-color: powderblue">
    <h1 class="title">Hello, @Model.CurrentUser !</h1>
    <p><a class="ManageButton" asp-controller="Volunteer" asp-action="ManageVolunteer" asp-route-login="@Model.CurrentUser">Manage Volunteers</a></p>
    <p>OR</p>
    <p><a class="ManageButton" asp-controller="Volunteer" asp-action="ManageOpportunity">Manage Opportunities</a></p>
</body>
</html>